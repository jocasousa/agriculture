{
  "rootDir": ".",
  "preset": "ts-jest",
  "testEnvironment": "node",
  "moduleFileExtensions": [
    "js",
    "ts",
    "json"
  ],
  "testMatch": [
    "**/test/e2e/**/*.e2e-spec.ts"
  ],
  "transform": {
    "^.+\\\\.ts$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "collectCoverage": false
}